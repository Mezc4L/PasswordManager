        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordManager"
        mc:Ignorable="d"
        Title="TestConsole" Height="506.962" Width="613.291">
    <Grid>
        <TextBox x:Name="tbConsole" HorizontalAlignment="Left" Height="324" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="585" AcceptsReturn="True"/>
