        <Button x:Name="btEncrypt" Content="Encrypt" HorizontalAlignment="Left" Margin="10,395,0,0" VerticalAlignment="Top" Width="75" Click="btEncrypt_Click"/>
        <Button x:Name="btDecrypt" Content="Decrypt" HorizontalAlignment="Left" Margin="123,395,0,0" VerticalAlignment="Top" Width="75" Click="btDecrypt_Click"/>

    </Grid>
